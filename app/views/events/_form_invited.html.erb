<%# invited label button%>
<a data-toggle="collapse" id="invited_label_button" data-toggle="collapse" data-target="#invited_container" aria-expanded="true" href="#invited_container">
<div class="setting-label" onclick="toggle_label_button('#information_label_button', 0)">People Involved</div>
</a>

<%# invited form %>
<div id="invited_container" class="collapse in" aria-expanded="true">
  <div id="the-basics" class="form-group">
    <%= f.label :invite_user_name, "Invite Users" %>
    <%= f.text_field :invite_user_name, class: "typeahead", type: :text, placeholder: "Type a User's name you'd like to invite", id: "target", data: "event" %>
  </div>
  <div id = 'user_results'>
      <p> List of finds </p>
  </div>
  <div id="event_invited_users_form">
    <p> Manage invited users to event </p>
    <% if @event_invited_users != nil %>
      <% i=0 %>
      <%= f.fields_for :event_invited_users do |builder| %>
        <%= render "form_user_fields", f: builder , i: i %>
        <% i+=1 %>
      <% end %>
    <% end %>
  </div>
</div>