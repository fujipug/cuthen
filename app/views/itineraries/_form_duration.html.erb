<%# duration label button%>
<a data-toggle="collapse" id="duration_label_button" data-toggle="collapse" data-target="#duration_container" aria-expanded="true" href="#duration_container">
<div class="setting-label" onclick="toggle_label_button('#information_label_button', 0)">Itinerary Duration</div>
</a>

<%# duration form %>
<div id="duration_container" class="collapse in" aria-expanded="true">
  <%# Calendar inputs %>
  <div class="row">
    <div class="col-sm-6">
      <div class="form-group">
        <div class='input-group date' id='datetimepicker1'>
          <%= f.label :startdate, "Start Date" %>
          <%= f.text_field :startdate, class: "datetime-form-control", placeholder: "--/--/---- --:-- --" %>
        </div>
      </div>
    </div>
    <div class="col-sm-6">
      <div class="form-group">
        <div class='input-group date' id='datetimepicker2'>
          <%= f.label :enddate, "End Date" %>
          <%= f.text_field :enddate, class: "datetime-form-control", placeholder: "--/--/---- --:-- --" %>
        </div>
      </div>
    </div>
  </div>
</div>