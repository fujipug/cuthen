<div class = "row">
  <div class="col-md-4">
    <button type="button" class="btn btn-default" onclick="showList()">
      <span class="glyphicon glyphicon-th-list" aria-hidden="true"></span> List
    </button>
    <button type="button" class="btn btn-default" onclick="showCalendar()">
      <span class="glyphicon glyphicon-calendar" aria-hidden="true"></span> Calendar
    </button>
  </div>
  <div class="col-md-4 col-md-offset-4">
    <a href = "/itineraries/new" class="btn btn-success" role = "button">+Create Itinerary
    </a>
  </div>
</div>
<% i = 0 %>
<% @itineraries.each do |itinerary| %>
  <input type="checkbox" checked="true" id="itinerary_hide_toggle<%= itinerary.id %>" onclick="hide_itinerary(<%= itinerary.id %>)">
  Itinerary: <%= itinerary.id %> | <%= itinerary.name %><br>
  <% @itinerary_event_count[i].times do |j| %>
    <input type="checkbox" checked="true" class="itinerary_hide_toggle<%= itinerary.id %>" id="event_hide_toggle<%= itinerary.id %>_<%= @events[j].id %>" onclick="hide_event(<%= itinerary.id %>, <%= @events[j].id %>)">
    Event: <%= @events[j].id %> | <%= @events[j].name %><br>
  <% end %>
  <% i += 1 %>
<% end %>
